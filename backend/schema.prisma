// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id       String @id @default(cuid())
  name     String @default("")
  email    String @unique @default("")
  password String
}

model Page {
  id      String   @id @default(cuid())
  page    String   @default("")
  status  String?  @default("draft")
  content Json     @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  seo     SEO_Tag? @relation("Page_seo", fields: [seoId], references: [id])
  seoId   String?  @map("seo")

  @@index([seoId])
}

model Job {
  id          String    @id @default(cuid())
  company     String    @default("")
  position    String    @default("")
  type        String?   @default("draft")
  contract    Boolean   @default(false)
  startDate   DateTime?
  endDate     DateTime?
  isCurrent   Boolean   @default(false)
  description String    @default("")
}

model Project {
  id          String  @id @default(cuid())
  title       String  @default("")
  subtitle    String  @default("")
  description String  @default("")
  type        String? @default("personal")
}

model SEO_Tag {
  id             String @id @default(cuid())
  page           String @default("")
  metaTitle      String @default("")
  metDescription String @default("")
  from_Page_seo  Page[] @relation("Page_seo")
}