{
  "name": "leodube.ca",
  "version": "1.0.0",
  "private": true,
  "description": "Personal website and portfolio of Leo Dube.",
  "author": "Leo Dube",
  "scripts": {
    "build": "concurrently -c \"bgBlue.bold,bgMagenta.bold\" \"yarn:build:*\" --kill-others --group --timings",
    "build:backend": "cd backend && yarn build",
    "build:frontend": "gatsby build",
    "dev": "concurrently -c \"bgBlue.bold,bgMagenta.bold\" \"yarn:dev:*\" --kill-others",
    "dev:backend": "cd backend && yarn dev",
    "dev:frontend": "gatsby develop",
    "serve": "concurrently -c \"bgBlue.bold,bgMagenta.bold\" \"yarn:serve:*\" --group",
    "serve:backend": "cd backend && yarn start",
    "serve:frontend": "gatsby serve",
    "start": "gatsby develop",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leodube/leodube.ca.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leodube/leodube.ca/issues"
  },
  "homepage": "https://github.com/leodube/leodube.ca#readme",
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^2.0.4",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "framer-motion": "^6.5.1",
    "gatsby": "^4.22.0",
    "gatsby-source-graphql": "^4.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.16",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.0.2",
    "babel-loader": "^8.2.5",
    "babel-preset-gatsby": "^2.22.0",
    "dotenv": "^16.0.2",
    "jest": "^29.0.2",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "storybook-addon-gatsby": "^0.0.5",
    "typescript": "^4.8.2"
  }
}
